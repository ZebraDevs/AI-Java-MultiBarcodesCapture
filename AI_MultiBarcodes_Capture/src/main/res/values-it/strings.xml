    <string name="resolution_medium">1280 x 1280 (medio)</string>
    <string name="resolution_medium_description">Velocità e precisione bilanciate</string>
    <string name="resolution_large">1600 x 1600 (grande)</string>
    <string name="resolution_large_description">Più lento; usa per codici a barre piccoli, danneggiati o distanti</string>
